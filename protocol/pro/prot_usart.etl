bitrl protocol prot_usart {
    segment type {parser: 'uint8', autovalue: 0x05}
    segment path {parser: 'uint8', autovalue: 0}
    segment check {parser: 'uint8', autovalue: 0}
    segment baud_rate {parser: 'uint8', autovalue: 0}
    segment data_bit {parser: 'uint8', autovalue: 0x08}
    segment stop_bit {parser: 'uint8', autovalue: 0}
    segment data_len {parser: 'uint8', autovalue: 0}
}
